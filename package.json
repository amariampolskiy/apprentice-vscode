{
	"name": "theme-apprentice",
	"version": "1.0.2",
	"displayName": "Apprentice",
	"description": "Apprentice Theme. A dark low contrast theme.",
	"publisher": "apprentice-theme",
	"license": "MIT",
	"scripts": {
		"dev": "node ./scripts/dev.js",
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/apprentice.vsix",
		"vscode:prepublish": "npm run build"
	},
	"maintainers": [
		"Mariampolskiy Anton <amariampolskiy@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/amariampolskiy/apprentice-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/amariampolskiy/apprentice-vscode/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"apprentice",
		"dark",
		"theme",
		"color-theme"
	],
	"icon": "logo-white.png",
	"galleryBanner": {
		"color": "#bcbcbc",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Apprentice",
				"uiTheme": "vs-dark",
				"path": "./theme/apprentice.json"
			},
			{
				"label": "Apprentice Bright",
				"uiTheme": "vs-dark",
				"path": "./theme/apprentice-bright.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^3.13.1",
		"prettier": "^1.17.0",
		"tinycolor2": "^1.4.1"
	}
}
